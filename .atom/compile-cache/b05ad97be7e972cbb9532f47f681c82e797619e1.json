{".text.html":{"Plachehoder Label":{"prefix":"ionic-css-form-placeholder","body":"<div class=\"list\">\n  <label class=\"item item-input\">\n    <input type=\"text\" placeholder=\"${1:First Name}\">\n  </label>\n  <label class=\"item item-input\">\n    <input type=\"text\" placeholder=\"${2:Last Name}\">\n  </label>\n  <label class=\"item item-input\">\n    <textarea placeholder=\"${3:Comments}\"></textarea>\n  </label>\n</div>"},"Floating Label":{"prefix":"ionic-css-form-floating","body":"<div class=\"list\">\n  <label class=\"item item-input item-floating-label\">\n    <span class=\"input-label\">${1:First Name}</span>\n    <input type=\"text\" placeholder=\"${2:First Name}\">\n  </label>\n  <label class=\"item item-input item-floating-label\">\n    <span class=\"input-label\">${3:Last Name}</span>\n    <input type=\"text\" placeholder=\"${4:Last Name}\">\n  </label>\n  <label class=\"item item-input item-floating-label\">\n    <span class=\"input-label\">${5:Email}</span>\n    <input type=\"text\" placeholder=\"${6:Email}\">\n  </label>\n</div>"},"Stacked Label":{"prefix":"ionic-css-form-stacked","body":"<div class=\"list\">\n  <label class=\"item item-input item-stacked-label\">\n    <span class=\"input-label\">${1:First Name}</span>\n    <input type=\"text\" placeholder=\"${2:John}\">\n  </label>\n  <label class=\"item item-input item-stacked-label\">\n    <span class=\"input-label\">${3:Last Name}</span>\n    <input type=\"text\" placeholder=\"${4:Suhr}\">\n  </label>\n  <label class=\"item item-input item-stacked-label\">\n    <span class=\"input-label\">${5:Email}</span>\n    <input type=\"text\" placeholder=\"${6:john@suhr.com}\">\n  </label>\n</div>"},"Inline Label":{"prefix":"ionic-css-form-inline","body":"<div class=\"list\">\n  <label class=\"item item-input\">\n    <span class=\"input-label\">${1:Username}</span>\n    <input type=\"text\">\n  </label>\n  <label class=\"item item-input\">\n    <span class=\"input-label\">${2:Password}</span>\n    <input type=\"password\">\n  </label>\n</div>"},"Form in Header":{"prefix":"ionic-css-form-header","body":"<div class=\"bar bar-header item-input-inset\">\n  <label class=\"item-input-wrapper\">\n    <i class=\"icon ion-ios7-search placeholder-icon\"></i>\n    <input type=\"search\" placeholder=\"${1:Search}\">\n  </label>\n  <button class=\"button button-clear\">\n    ${2:Cancel}\n  </button>\n</div>"},"Form with Icons":{"prefix":"ionic-css-form-icon","body":"<div class=\"list list-inset\">\n  <label class=\"item item-input\">\n    <i class=\"icon ion-search placeholder-icon\"></i>\n    <input type=\"text\" placeholder=\"${1:Search}\">\n  </label>\n</div>"},"Inset Form":{"prefix":"ionic-css-form-inset","body":"<div class=\"list list-inset\">\n  <label class=\"item item-input\">\n    <input type=\"text\" placeholder=\"${1:First Name}\">\n  </label>\n  <label class=\"item item-input\">\n    <input type=\"text\" placeholder=\"${2:Last Name}\">\n  </label>\n</div>"}}}